  },
  "homepage": "https://github.com/pennlabs/penncentral#readme",
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "async": "^2.6.1",
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "^1.8.0",

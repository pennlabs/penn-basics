    "@types/styled-components": "^4.4.2",
    "chai": "^4.1.2",
    "concurrently": "^4.1.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "mocha": "^4.0.1",
    "prettier": "^1.19.1",
    "typescript": "^3.7.4"
